{
  "name": "react-and-express-startproject",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \\\"Error: no test specified\\\" && exit 1",
    "build": "npm run build:server && npm run build:client",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "dev": "concurrently npm:dev:client npm:dev:server",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "start": "cd server && npm start",
    "postinstall": "npm run install:client && npm run install:server",
    "install:client": "cd client && npm install --include=dev",
    "install:server": "cd server && npm install",
    "format": "prettier --write **/*.{js,jsx,ts,tsx}",
    "format:check": "prettier --check **/*.{js,jsx,ts,tsx}",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojibhashemi123/react-and-express-startproject.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mojibhashemi123/react-and-express-startproject/issues"
  },
  "homepage": "https://github.com/mojibhashemi123/react-and-express-startproject#readme",
  "devDependencies": {
    "concurrently": "^7.1.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.2"
  },
  "engines": {
    "node": "16.13.2",
    "npm": "8.3.1"
  }
}
